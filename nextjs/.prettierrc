{
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "^(next|react)(/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "^@(src|hooks)/(.*)$",
    "^@(components|styles)/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
